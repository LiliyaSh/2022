#include <math.h>
#include <iostream>
using namespace std;

double dy(double x, double y) {
	return 0.276 * (x * x + sin(1.6 * x)) + 0.988 * y;
}

int main() {
	setlocale(LC_ALL, "Russian");
	double x = 0.2;
	double y = 0.25;
	double y1, dy1, hy = 0;
	for (int i = 0; i < 11; i++) {
		double xh = x + 0.05;
		y1 = y + 0.05 * y;
		hy = dy(xh, y1) * 0.1;
		cout << "x" << i << ": " << x << "   y" << i << ": " << y << endl;
		y = y + hy;
		x += 0.1;
	}
	return 0;
}
